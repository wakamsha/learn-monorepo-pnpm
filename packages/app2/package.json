{
  "name": "@learn-monorepo-pnpm/app2",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "scripts": {
    "type-check": "tsc --noEmit",
    "typegen:scss": "typed-scss-modules src",
    "reset": "rm -rf ./.next && rm -rf ./out",
    "dev": "next dev -p 3001",
    "start": "pnpm reset && run-p \"type-check --watch\" \"typegen:scss -w\" \"dev\"",
    "build": "pnpm reset && next build && next export"
  },
  "dependencies": {
    "@learn-monorepo-pnpm/core": "1.0.0",
    "next": "13.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "18.13.0",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "sass": "1.58.0",
    "typed-scss-modules": "7.0.2"
  }
}

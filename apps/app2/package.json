    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@vitejs/plugin-react": "3.1.0",
    "npm-run-all": "4.1.5",
    "sass": "1.58.0",
    "typed-scss-modules": "7.1.0",
    "vite": "4.1.1"
  }
}
